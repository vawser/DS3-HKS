        if category == 1 then
            SetVariable("RightArmAddWeight_a02", 1)
            SetVariable("RightArmAddWeight_a03", 0)
        elseif category == 2 then
            SetVariable("RightArmAddWeight_a02", 0)
            SetVariable("RightArmAddWeight_a03", 1)
        else
            SetVariable("RightArmAddWeight_a02", 0)
            SetVariable("RightArmAddWeight_a03", 0)
