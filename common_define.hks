function CalcArmTwistAngle(kind, cur_angle, is_both)
    local f12_local0 = {0, 0, 0, 0}
    local f12_local1 = {0, 0, 0, 0}
    if GetVariable("DebugArmTwistWeaponCategory") ~= 0 then
        local f12_local2 = ArmTwistWeaponCategory
        local f12_local3 = {}
        local f12_local4 = GetVariable("DebugArmTwistWeaponCategory")
        local f12_local5 = GetVariable("DebugArmTwistWristFront")
        local f12_local6 = GetVariable("DebugArmTwistWristSide")
        local f12_local7 = GetVariable("DebugArmTwistUpperArmFront")
        local f12_local8 = GetVariable("DebugArmTwistUpperArmSide")
        f12_local2[1] = f12_local4
    else
        ArmTwistWeaponCategory[1] = {999, 0, 0, 0, 0}
    end
    if is_both == FALSE then
        for f12_local2 = 1, #ArmTwistWeaponCategory, 1 do
            if kind == ArmTwistWeaponCategory[f12_local2][1] then
                for f12_local6 = 1, 4, 1 do
                    f12_local0[f12_local6] = ArmTwistWeaponCategory[f12_local2][f12_local6 + 1]
                end
                break

            end
        end
    end
    for f12_local2 = 1, 4, 1 do
        if f12_local0[f12_local2] ~= 0 or cur_angle[f12_local2] ~= 0 then
            break
        end
        if f12_local2 == 4 then
            return f12_local1
        end
    end
    local f12_local2 = {0, 0, 0, 0}
    if 0 < g_TimeActEditor_00 then
        for f12_local3 = 1, 4, 1 do
            f12_local2[f12_local3] = f12_local0[f12_local3] * g_TimeActEditor_00
        end
    elseif 0 < g_TimeActEditor_02 then
        for f12_local3 = 1, 4, 1 do
            f12_local2[f12_local3] = f12_local0[f12_local3] * g_TimeActEditor_02
        end
    end
    if GetVariable("LocomotionState") == PLAYER_STATE_MOVE then
        f12_local2[3] = kind
        f12_local2[4] = kind
    end
    for f12_local3 = 1, 4, 1 do
        if f12_local2[f12_local3] ~= cur_angle[f12_local3] then
            break
        end
        if f12_local3 == 4 then
            f12_local1 = cur_angle
            return f12_local1
        end
    end
    local max_converge_rate = 90
    local base_angle = math.max(math.abs(f12_local0[1]), math.abs(f12_local0[2]), math.abs(f12_local0[3]), math.abs(f12_local0[4]))
    for f12_local5 = 1, 4, 1 do
        local converge_rate = max_converge_rate * (math.abs(f12_local0[f12_local5]) / base_angle)
        local tmp = ConvergeValue(f12_local2[f12_local5], cur_angle[f12_local5], converge_rate, converge_rate)
        f12_local1[f12_local5] = tmp
    end
    return f12_local1



end

